function fullname(firstName, lastName) {
    return `${firstName} ${lastName}`
}

const addNumbers = (num, num2) => {
    return num + num2
}

console.log(addNumbers(1,2))

function reverseArray(arr) {
    const ret = [];
    for(var i = arr.length - 1; i >= 0; i--) {
        ret.push(arr[i])
    }
    return ret
}

console.log(reverseArray([1,2,3]))